<p>hello</p>

<div class="container">
  <% @location.plants.each do |plant| %>
    <div class="card-info p-3">
      <p><strong><%= plant.name%></strong></p>
      <p><strong>age<%=plant.age %></strong></p>
      <p><strong>level<%= plant.level%></strong></p>
    </div>
  </div>
  <% end %>
  <div class="container-footer">
    <ul>
      <%= link_to root_path do %>
        <i class="fa fa-user-plus"></i>
      <% end %>
    </ul>
  </div>
</div>
